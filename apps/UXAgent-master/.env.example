# =============================================================================
# BROWSER MODE
# =============================================================================
# Set to "browserbase" for cloud browser sessions, "local" for local Chromium
BROWSER_MODE=local

# =============================================================================
# BROWSERBASE CONFIGURATION (required when BROWSER_MODE=browserbase)
# =============================================================================
# Get these from https://browserbase.com/dashboard
BROWSERBASE_API_KEY=your_api_key_here
BROWSERBASE_PROJECT_ID=your_project_id_here

# =============================================================================
# LLM API KEYS (choose one or more)
# =============================================================================
# Provider: "gemini" (default), "anthropic", "openai", or "aws"
LLM_PROVIDER=gemini

# Gemini API key (default provider - free tier available)
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Anthropic API key for Claude (optional)
# ANTHROPIC_API_KEY=sk-ant-xxx

# OpenAI API key (optional)
# OPENAI_API_KEY=sk-xxx

# AWS Bedrock (alternative to direct API keys)
# AWS_ACCESS_KEY_ID=xxx
# AWS_SECRET_ACCESS_KEY=xxx

# =============================================================================
# BROWSER SETTINGS
# =============================================================================
# Run browser in headless mode (only applies to local mode)
HEADLESS=true

# =============================================================================
# OPTIONAL SETTINGS
# =============================================================================
# Debug mode for verbose logging
# DEBUG=true

# Use Stagehand for browser automation (natural language actions)
# When true, uses StagehandEnv instead of WebAgentEnv
# USE_STAGEHAND=true
